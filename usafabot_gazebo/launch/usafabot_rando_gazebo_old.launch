      <arg name="init_pose" value="-x 0.1 -y 0.1 -z 0 -R 0 -P 0 -Y 1.5708" />
      <arg name="robot_name"  value="usafabot0" />
    </include>
	<node
	name="controller"
	pkg="capstone21-22"
